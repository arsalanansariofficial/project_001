generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model url {
  id        String   @id @default(uuid())
  fullUrl   String   @unique
  shortUrl  String
  clicked   Int      @default(0)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
